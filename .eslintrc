{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "ecmaFeatures": {
    "impliedStrict": true
  },
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "comma-dangle": [2, "always-multiline"],
    "comma-style": [2, "last"],
    "consistent-this": [2, "self"],
    "constructor-super": 2,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "generator-star-spacing": [2, { "before": false, "after": true }],
    "handle-callback-err": 2,
    "indent": [2, "tab", { "SwitchCase": 2, "MemberExpression": 0 }],
    "keyword-spacing": [2, { "after": true }],
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-div-regex": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2, { "boolean": false, "number": true, "string": true }],
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-negated-condition": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-globals": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-sync": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-var": 2,
    "no-warning-comments": 2,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "operator-assignment": [2, "always"],
    "operator-linebreak": [2, "after"],
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
    "require-yield": 2,
    "semi": [2, "always"],
    "strict": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
  }
}
